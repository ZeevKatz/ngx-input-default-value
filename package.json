    "@types/lodash-es": "^4.14.123",
    "@types/node": "^10.1.4",
    "jest": "^24.7.1",
    "ng-packagr": "^5.1.0",
    "rxjs": "^6.5.1",
    "ts-jest": "^24.0.2",
    "tsickle": "^0.33.1",
    "tslib": "^1.9.3",
    "typescript": "3.1.3",
    "zone.js": "~0.8.26"
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
